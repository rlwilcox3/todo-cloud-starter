{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "todo-rabbitmq",
        "image": "rabbitmq:3.12.13-management",
        "hostname": "todo-rabbitmq",
        "memory": 200,
        "essential": false
      },
      {
        "name": "todo-view",
        "image": "rlwilcox3/todo-view:1.0",
        "hostname": "todo-view",
        "memory": 768,
        "essential": false
      },
      {
        "name": "todo-proxy",
        "image": "rlwilcox3/todo-proxy:1.0",
        "memory": 128,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80
          }
        ],
        "links": ["todo-view"]
      }
    ]
  }